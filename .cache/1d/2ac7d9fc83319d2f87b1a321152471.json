n    color: #cf5d53;\r\n  }\r\n  .green {\r\n    color: #41d18b;\r\n  }\r\n  .blue {\r\n    color: #51aced;\r\n  }\r\n\r\n  .textBlock video {\r\n    width: 100%;\r\n  }\r\n\r\n  .button {\r\n    width: 37rem;\r\n    font-family: $mainFont;\r\n    font-size: 1.75rem;\r\n    background-color: $red;\r\n    color: $yellow;\r\n    margin: 3rem auto 2rem;\r\n    padding: 1rem;\r\n    border-radius: 0.4rem;\r\n    -moz-outline-radius: 0.4rem;\r\n    font-style: normal;\r\n  }\r\n  .button:hover {\r\n    // background-color: $yellow;\r\n    background-color: inherit;\r\n    color: $red;\r\n    outline: 0.2rem solid $mohogany;\r\n    transition: ease-in 0.15s;\r\n    border-radius: 0.4rem;\r\n  }\r\n  \r\n\r\n  h3 {\r\n    color: $mohogany;\r\n  }\r\n\r\n\r\n  .row {\r\n    display: grid;\r\n    grid-gap: 1rem;\r\n    //margin-top: 2rem;\r\n    //margin-bottom: 2rem;\r\n  }\r\n  .textBlock {\r\n    padding-top: 0;\r\n    grid-gap: 3rem;\r\n    color: black;\r\n    text-align: left;\r\n  }\r\n  .space {\r\n    display: grid;\r\n    grid-gap: 1rem;\r\n    margin-bottom: 2rem;\r\n  }\r\n\r\n  .textBlock p {\r\n    color: black;\r\n  }\r\n\r\n  .img-carousel {\r\n    display: flex;\r\n    justify-content: space-around;\r\n    flex-direction: row;\r\n  }\r\n\r\n  .img-width {\r\n    width: 50% !important;\r\n    padding: 1rem;\r\n  }\r\n\r\n  .vid-width {\r\n    width: 50%;\r\n    padding: 1rem;\r\n  }\r\n\r\n  .row-img {\r\n    padding: 2rem;\r\n    flex-basis: 100%;\r\n  }\r\n\r\n  .mySlides1,\r\n  .mySlides2,\r\n  .mySlides3 {\r\n    display: none;\r\n  }\r\n\r\n  /* Slideshow container */\r\n  .slideshow-container {\r\n    max-width: 100%;\r\n    position: relative;\r\n    margin: auto;\r\n  }\r\n\r\n  /* Next & previous buttons */\r\n  .prev,\r\n  .next {\r\n    cursor: pointer;\r\n    position: absolute;\r\n    top: 50%;\r\n    width: auto;\r\n    padding: 16px;\r\n    font-weight: bold;\r\n    font-size: 18px;\r\n    transition: 0.6s ease;\r\n    border-radius: 0 3px 3px 0;\r\n    user-select: none;\r\n  }\r\n\r\n  /* Position the \"next button\" to the right */\r\n  .next {\r\n    right: 0;\r\n    border-radius: 3px 0 0 3px;\r\n  }\r\n\r\n  .prev {\r\n    left: 0;\r\n    border-radius: 3px 0 0 3px;\r\n  }\r\n\r\n  /* On hover, add a grey background color */\r\n  .prev:hover,\r\n  .next:hover {\r\n    background-color: #f1f1f1;\r\n    color: black;\r\n  }\r\n\r\n  .row {\r\n    display: flex;\r\n    justify-content: space-between;\r\n  }\r\n\r\n  .row-reverse {\r\n    display: flex;\r\n    flex-direction: row-reverse;\r\n  }\r\n\r\n  .row-padding {\r\n    padding: 3rem;\r\n  }\r\n\r\n  .columnleft {\r\n    display: grid;\r\n    grid-template-columns: 1fr 1fr;\r\n    padding: 1%;\r\n  }\r\n\r\n  .right {\r\n    float: right !important;\r\n    width: 48%;\r\n    //padding-left: 10%;\r\n  }\r\n\r\n  .grid-container {\r\n    display: grid;\r\n    grid-template-columns: 1fr 1fr 1fr;\r\n    padding: 10px;\r\n    grid-column-gap: 2rem;\r\n    grid-row-gap: 2rem;\r\n  }\r\n\r\n  .grid-item {\r\n    background-color: #fff8ea;\r\n    padding: 2rem;\r\n    text-align: center;\r\n  }\r\n\r\n  .grid-dark {\r\n    background-color: #ffedc5;\r\n    padding: 0.5rem;\r\n    font-family: \"Raleway\", sans-serif;\r\n    font-size: 1.75rem;\r\n    color: #cf5d53;\r\n    text-align: center;\r\n  }\r\n\r\n  .grid-light {\r\n    padding: 1rem;\r\n  }\r\n\r\n  .art-container {\r\n    display: flex;\r\n    padding: 10px;\r\n    grid-column-gap: 2rem;\r\n    grid-row-gap: 2rem;\r\n  }\r\n\r\n  .numbertext {\r\n    color: #913132;\r\n    padding: 8px 12px;\r\n    position: absolute;\r\n    top: 0;\r\n  }\r\n\r\n  .cursor {\r\n    cursor: pointer;\r\n  }\r\n\r\n  .close {\r\n    color: white;\r\n    position: absolute;\r\n    top: 0;\r\n    right: 0;\r\n    margin: 2rem;\r\n    font-weight: bold;\r\n    fill: #ffedc5;\r\n  }\r\n\r\n  .close:hover,\r\n  .close:focus {\r\n    color: #999;\r\n    text-decoration: none;\r\n    cursor: pointer;\r\n  }\r\n\r\n  .popup-img {\r\n    max-width: 30rem;\r\n  }\r\n\r\n  .modal {\r\n    display: none;\r\n    position: fixed;\r\n    z-index: 1;\r\n    padding-top: 6rem;\r\n    left: 0;\r\n    top: 0;\r\n    width: 100%;\r\n    height: 100%;\r\n    overflow: scroll;\r\n    background-color: rgba(0, 0, 0, 0.7);\r\n  }\r\n\r\n  .modal-content {\r\n    position: relative;\r\n    background-color: #fefefe;\r\n    margin: auto;\r\n    padding: 5rem;\r\n    max-width: 100rem;\r\n  }\r\n  .sub{\r\n    font-family: \"Playfair Display\", serif;\r\n    font-size: 2.5rem;\r\n    color: #913132;\r\n    padding-top: 0.5rem;\r\n  }\r\n\r\n  @media only screen and (max-width: 700px) {\r\n    .row {\r\n      display: flex;\r\n      flex-direction: column;\r\n    }\r\n    .row-reverse {\r\n      display: flex;\r\n      flex-direction: column;\r\n    }\r\n    .grid-container {\r\n      grid-template-columns: 1fr 1fr;\r\n    }\r\n    .art-container {\r\n      display: flex;\r\n      flex-direction: column;\r\n    }\r\n  }\r\n\r\n  @media only screen and (max-width: 400px) {\r\n    .row {\r\n      display: flex;\r\n      flex-direction: column;\r\n    }\r\n    .row-reverse {\r\n      display: flex;\r\n      flex-direction: column;\r\n    }\r\n    .grid-container {\r\n      grid-template-columns: 1fr;\r\n    }\r\n    .art-container {\r\n      display: flex;\r\n      flex-direction: column;\r\n    }\r\n  }\r\n\r\n}\r\n","scss\\_variables.scss":"$mohogany: #913132;\r\n$red: #cf5d53;\r\n$yellow: #ffedc5;\r\n$lightYellow: #fff8ea;\r\n$blue: #134360;\r\n$brown: #cd9f7e;\r\n\r\n$mainFont: \"Playfair Display\", serif;\r\n$secondFont: \"Raleway\", sans-serif;\r\n\r\n//for media queries\r\n$smallphone: 320px;\r\n$phone: 400px;\r\n$tablet: 700px;\r\n$laptop: 1000px;\r\n$desktop: 1400px;\r\n"},"lineCount":null}},"error":null,"hash":"1e18502fed40efa963bb93e45c25847d","cacheData":{"env":{}}}